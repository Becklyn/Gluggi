{% if options.includeNavigation %}
    <div class="gluggi-navigation"></div>
{% endif %}

{# @var elements \BecklynLayout\Entity\Element[] #}
{% for element in elements %}
    <div class="gluggi-element" id="{{ element.id }}">
        <a class="gluggi-element-title" href="">{{ element.fullTitle }}</a>
        <div class="gluggi-element-stage">
            {% include element.reference with {options: options} %}
        </div>
    </div>
{% endfor %}
